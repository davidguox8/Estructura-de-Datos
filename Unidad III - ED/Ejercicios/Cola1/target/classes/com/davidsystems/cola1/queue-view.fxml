<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.davidsystems.cola1.controller.QueueController"
            prefWidth="400" prefHeight="300">

    <VBox spacing="10" alignment="CENTER" layoutX="50" layoutY="50">
        <TextField fx:id="nameField" promptText="Enter a name" />
        <Button text="Add to Queue" onAction="#addToQueue"/>
        <ListView fx:id="queueList"/>
    </VBox>
</AnchorPane>
