<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.davidsystems.fila1.Controller.StackAddController"
            prefWidth="400" prefHeight="300">

    <VBox spacing="10" alignment="CENTER" layoutX="50" layoutY="50">
        <TextField fx:id="itemField" promptText="Enter item to push"/>
        <Button text="Push to Stack" onAction="#pushToStack"/>
        <ListView fx:id="stackList"/>
    </VBox>
</AnchorPane>
